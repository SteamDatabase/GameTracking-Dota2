<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/dashboard_page_post_game.vcss_c" />
	</styles>

	<Panel class="PlayerRowLegend">
		<Panel class="PlayerRowLegendContainer">
			<Label class="LegendKills KillsColumn" text="#dota_page_match_details_legend_kills" onmouseover="UIShowTextTooltipStyled( #dota_page_match_details_legend_kills_tooltip, ShortTextTooltip )" onmouseout="UIHideTextTooltip()" />
			<Label class="LegendDeaths DeathsColumn" text="#dota_page_match_details_legend_deaths" onmouseover="UIShowTextTooltipStyled( #dota_page_match_details_legend_deaths_tooltip, ShortTextTooltip )" onmouseout="UIHideTextTooltip()" />
			<Label class="LegendAssists AssistsColumn" text="#dota_page_match_details_legend_assists" onmouseover="UIShowTextTooltipStyled( #dota_page_match_details_legend_assists_tooltip, ShortTextTooltip )" onmouseout="UIHideTextTooltip()" />
			<Label class="LegendMMRChange MMRColumn" text="#dota_page_match_details_legend_mmr" onmouseover="UIShowTextTooltipStyled( #dota_page_match_details_legend_mmr_tooltip, ShortTextTooltip )" onmouseout="UIHideTextTooltip()" />
			<Panel class="NetWorthColumn LeftRightFlow"  onmouseover="UIShowTextTooltipStyled( #dota_page_match_details_legend_net_worth_tooltip, ShortTextTooltip )" onmouseout="UIHideTextTooltip()">
				<Panel class="GoldIcon"/>
				<Label class="LegendGold" text="#dota_page_match_details_legend_net_worth" />
			</Panel>
			<Label class="LegendItems ItemsColumn" text="#dota_page_match_details_legend_items" />
			<Label class="LegendBackpackItems BackpackItemsColumn" text="#dota_page_match_details_legend_backpack_items" />
			<Label class="LegendPermanentBuffs PermanentBuffsColumn" text="#dota_page_match_details_legend_permanent_buffs" />
			<Label class="LegendLastHits LastHitsColumn" text="#dota_page_match_details_legend_last_hits" onmouseover="UIShowTextTooltipStyled( #dota_page_match_details_legend_last_hits_tooltip, ShortTextTooltip )" onmouseout="UIHideTextTooltip()" />
			<Label class="BackSlash" text="/" />
			<Label class="LegendDenies DeniesColumn" text="#dota_page_match_details_legend_denies" onmouseover="UIShowTextTooltipStyled( #dota_page_match_details_legend_denies_tooltip, ShortTextTooltip )" onmouseout="UIHideTextTooltip()" />
			<Label class="LegendGoldPerMin GPMColumn" text="#dota_page_match_details_legend_gold_per_min" onmouseover="UIShowTextTooltipStyled( #dota_page_match_details_legend_gold_per_min_tooltip, ShortTextTooltip )" onmouseout="UIHideTextTooltip()" />
			<Label class="LegendXPPerMin XPMColumn" text="#dota_page_match_details_legend_xp_per_min" onmouseover="UIShowTextTooltipStyled( #dota_page_match_details_legend_xp_per_min_tooltip, ShortTextTooltip )" onmouseout="UIHideTextTooltip()" />
			<Label class="LegendHeroDamage HeroDamageColumn" text="#dota_page_match_details_legend_hero_damage" onmouseover="UIShowTextTooltipStyled( #dota_page_match_details_legend_hero_damage_tooltip, ShortTextTooltip )" onmouseout="UIHideTextTooltip()" />
			<Label class="LegendHeroHealing HeroHealingColumn" text="#dota_page_match_details_legend_hero_healing" onmouseover="UIShowTextTooltipStyled( #dota_page_match_details_legend_hero_healing_tooltip, ShortTextTooltip )" onmouseout="UIHideTextTooltip()" />
			<Label class="LegendTowerDamage TowerDamageColumn" text="#dota_page_match_details_legend_tower_damage" onmouseover="UIShowTextTooltipStyled( #dota_page_match_details_legend_tower_damage_tooltip, ShortTextTooltip )" onmouseout="UIHideTextTooltip()" />
		</Panel>			
	</Panel>
</root>
