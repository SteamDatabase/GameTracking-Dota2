<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/frontpage_shared.vcss_c" />
		<include src="s2r://panorama/styles/frontpage_default_button.vcss_c" />
	</styles>

	<DOTAFrontpageDefaultButton>
		<Image id="CellImage" class="FrontpageCellImage" scaling="stretch-to-cover-preserve-aspect" />
		<Label class="FrontpageCellHeader" text="{s:cell_header}">
			<Panel id="FrontpageCellTimer">
				<Label class="ExpirationLabel" text="#DOTA_CompTI10_FrontPage_TimerEnds" />
				<Label class="ExpirationLabel" html="true" text="#DOTA_CompTI10_FrontPage_TimerString" />
			</Panel>
		</Label>
		<Panel class="FrontpageCellTint" />
		<Panel class="FrontpageCellFooter">
			<Panel id="CellImageBlur" class="FrontpageCellImage"/>
			<Panel class="FrontpageCellFooterLabels">
				<Label class="FrontpageCellText" text="{s:cell_text}" html="true" />
				<Label class="FrontpageCellTitle" text="{s:cell_title}" html="true" />
			</Panel>
		</Panel>
	</DOTAFrontpageDefaultButton>

</root>
