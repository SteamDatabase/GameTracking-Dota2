    <snippets>

        <snippet name="BlessingSlot">
            <Panel class="BlessingSlot">
				<Button id="BlessingSlotButton" class="BlessingButton"/>
				<Panel id="BlessingSlotAttribColor" class="BlessingAttribColor"/>
				<Panel id="BlessingSlotIcon" class="BlessingIcon"/>
				<Panel id="BlessingSlotCost" class="BlessingCostContainer">
					<Image id="BlessingSlotFragmentIcon" class="ArcaneFragmentIcon" src="s2r://panorama/images/events/aghanim/aghanims_blessings_png.vtex" />
					<Label id="BlessingSlotCostText" class="BlessingCostText" text="#DOTA_TI10_EventGame_BlessingCost" />
				</Panel>
            </Panel>            
        </snippet>

        <snippet name="LeaderboardEntry">
        	<Panel class="LeaderListRow FullWidth">
				<Label class="LeaderListValue Rank MonoNumbersFont" text="#DOTA_TI10_EventGame_LeaderboardRank" />
				<Label class="LeaderListValue Depth MonoNumbersFont" text="#DOTA_TI10_EventGame_LeaderboardDepth" />
				<Label class="LeaderListValue Time MonoNumbersFont" text="#DOTA_TI10_EventGame_LeaderboardTime" />

				<Panel class="LeaderListValue Players">
        			<DOTAAvatarImage id="Player1" class="LeaderListAvatar" nocompendiumborder="true" />
        			<DOTAAvatarImage id="Player2" class="LeaderListAvatar" nocompendiumborder="true" />
        			<DOTAAvatarImage id="Player3" class="LeaderListAvatar" nocompendiumborder="true" />
        			<DOTAAvatarImage id="Player4" class="LeaderListAvatar" nocompendiumborder="true" />
        		</Panel>

				<Panel class="LeaderListValue Heroes">
					<DOTAHeroImage id="Hero1" class="HeroImage" heroimagestyle="icon" />
					<DOTAHeroImage id="Hero2" class="HeroImage" heroimagestyle="icon" />
					<DOTAHeroImage id="Hero3" class="HeroImage" heroimagestyle="icon" />
					<DOTAHeroImage id="Hero4" class="HeroImage" heroimagestyle="icon" />
				</Panel>

				<Button id="Watch" class="PopupButton" onactivate="DOTAWatchDownloadsWatch()">
					<Label text="#DOTA_WatchReplaysConextMenu_Watch"/>
				</Button>

        	</Panel>
        </snippet>

        <snippet name="LeaderboardHeader">
        	<Panel class="LeaderListHeader">
        		<Label class="LeaderListValue Rank"   	text="#DOTA_TI10_EventGame_Rank" />
        		<Label class="LeaderListValue Depth"  	text="#DOTA_TI10_EventGame_Depth" />
        		<Label class="LeaderListValue Time"   	text="#DOTA_TI10_EventGame_Time" />
        		<Label class="LeaderListValue Players"  text="#DOTA_TI10_EventGame_Team" />
        		<Label class="LeaderListValue Heroes" 	text="#DOTA_TI10_EventGame_Heroes" />	
        	</Panel>
        </snippet>

    </snippets>

	<DOTABPInternational2020EventGamePage class="DashboardPage BattlePassPage">
		<Panel id="CompendiumBody" class="CompendiumBody">
			<Panel class="GateGlow"/>

			<!-- Blessing Fragments Earned -->
			<Panel id="BlessingFragmentsContainer">
				<Panel class="BlessingFragmentsBank">
					<Panel class="CurrencyIcon BlessingFragments" />
					<Panel class="ValueContainer">
						<Label class="SectionText" text="#DOTA_TI10_EventGame_Blessing_Fragments_Earned" />
						<Label class="ValueText" text="{d:event_game_blessing_count}"/>
					</Panel>
				</Panel>
			</Panel>

			<!-- Blessing Debug -->
			<Panel id="BlessingDebugContainer" class="BlessingTreesDebugContainer TopBottomFlow">
				<Label class="BlessingTreesDebugTitle" text="#DOTA_TI10_EventGame_BlessingsDebugTitle"/>
					<Panel>
						<TextButton id="BlessingsResetBlessingsButton" class="BlessingsDebugButton" text="Reset Blessings" onactivate="DOTATI10EventGameResetBlessings()"/>
					</Panel>
				<Panel>
					<TextButton id="BlessingsGrantBlessingsButton" class="BlessingsDebugButton" text="Add Arcane Fragments" onactivate="DOTATI10EventGameGrantBlessingPoints()"/>
				</Panel>
			</Panel>
      
