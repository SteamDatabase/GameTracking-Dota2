								<Label class="ValueText" text="{d:num_games_played}" />
							</Panel>
							
							<!-- Best Run -->
							<Panel id="DifficultyContainer">
								<Label class="SectionText" text="#DOTA_TI10_EventGame_CurrentAscension"/>
								<Panel class="ValueContainer Difficulty">
									<Label class="ValueText" text="{s:curr_ascension_level_name}"/>
									<Label class="SecondaryValueText" text="#DOTA_TI10_EventGame_DeepestJourney"/>
								</Panel>
							</Panel>					
							
							<!-- Battle Pass Points Earned -->
							<Panel id="PointsEarnedSection">
								<Panel class="PointsEarnedContainer">
									<Panel class="PointsEarnedLabel">
										<Label class="SectionText" text="#DOTA_TI10_EventGame_BP_Earned"/>
										<Panel id="InfoIcon" class="InfoIcon" onmouseover="UIShowTextTooltip( '#DOTA_TI10_EventGame_BPInfoTooltip' )" onmouseout="UIHideTextTooltip()" />
									</Panel>
									<Panel class="ValueContainer">
										<Label class="ValueText Ti10" text="#DOTA_TI10_EventGame_BP_EarnedValue"/>
										<Panel class="CurrencyIcon BattlePoints" />
									</Panel>
								</Panel>

								<!-- Battle Pass Progress Bar -->
								<Panel class="ProgressBarContainer">
									<ProgressBar id="PointsProgressBar" class="EventGameProgressBar" value="0"/>
								</Panel>
							</Panel>
							
													
							<!-- Blessing Fragment Bonus -->
							<Panel id="PremiumPointsEarnedSection">
								<Panel class="PointsEarnedContainer">
									<Panel class="PointsEarnedLabel">
										<Label class="SectionText" text="#DOTA_TI10_EventGame_AF_Used"/>
										<Panel id="InfoIcon" class="InfoIcon" onmouseover="UIShowTextTooltip( '#DOTA_TI10_EventGame_ArcaneFragmentsInfoTooltip' )" onmouseout="UIHideTextTooltip()" />
									</Panel>
									<Panel class="ValueContainer">
										<Label class="ValueText" text="#DOTA_TI10_EventGame_AF_UsedValue"/>
										<Panel class="CurrencyIcon BlessingFragments" />
