<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/popups/popups_shared.vcss_c" />
		<include src="s2r://panorama/styles/popups/popup_high_priority_queue_warning.vcss_c" />
	</styles>

	<Panel class="PopupPanel Hidden" popupbackground="dim" oncancel="UIPopupButtonClicked()">
		<Panel class="TitleRow">
			<Label class="PopupTitle" text="#DOTA_RRParty_NonEligibleWarningTitle" />
		</Panel>

		<Label id="Description" class="Description" html="true" text="#DOTA_RRParty_NonEligibleWarning_Plural" />

		<Panel class="PopupButtonRow">
			<Panel class="SlowButton ButtonRow">
				<Button class="PopupButton TopBottomFlow SlowQueue" onactivate="UIPopupButtonClicked(DOTAHPQWarningResult(1))">
					<Panel class="PopupDescription">
						<Label text="#DOTA_RRParty_QueueNormalPriority" />
					</Panel>
				</Button>
				<Label class="SupplementText" text="#DOTA_RRParty_QueueNormalPriority_Supplement1" />
			</Panel>

			<Panel class="FastButton ButtonRow">	
				<Button class="PopupButton TopBottomFlow QueueAllRoles" onactivate="UIPopupButtonClicked(DOTAHPQWarningResult(2))">
					<Panel class="PopupDescription">
						<Label text="#DOTA_RRParty_EditPartyRoles" />
					</Panel>
				</Button>
			</Panel>
		</Panel>
	</Panel>
</root>
