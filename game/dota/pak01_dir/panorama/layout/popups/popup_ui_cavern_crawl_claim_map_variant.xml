<root>
	<styles>
		<include src="s2r://panorama/styles/gamestyles.vcss_c" />
		<include src="s2r://panorama/styles/popups/popups_shared.vcss_c" />
		<include src="s2r://panorama/styles/battle_pass/current_battle_pass.vcss_c" />
		<include src="s2r://panorama/styles/popups/popup_ui_cavern_crawl_claim_map_variant.vcss_c" />
	</styles>

	<DOTAPopupUICavernCrawlClaimMapVariant class="PopupPanel Hidden" popupbackground="dim" >

		<Panel id="Title">
			<Panel id="TitleBackground">
				<Button id="CancelButton" class="CloseButton" onactivate="UIPopupButtonClicked()" />
				<Panel class="TitleContainer" >
					<Panel class="CavernCrawlLogo" />
					<Label class="TitleLabel" text="#DOTACavernCrawl_Cavern_MapVariantClaimedTitle" />
				</Panel>
			</Panel>
		</Panel>

		<Panel id="Body">
			<Panel class="TopRow">
				<Panel class="HeroContainer">
					<Panel class="HeroCavernImage"/>
					<Panel class="InfoContainer">
						<Label class="InfoLabel Lablel1" text="#DOTACavernCrawl_MapVariantClaimed_DescLine1" />
					</Panel>
				</Panel>
				<Panel class="ButtonContainer">
					<Panel class="ButtonPreview"/>
					<!--<Label class="CavernCrawlTitle" text="#DOTACavernCrawl_PregameTitle" />-->
					<Panel class="InfoContainer">
						<Label class="InfoLabel Label2" text="#DOTACavernCrawl_MapVariantClaimed_DescLine2" />
					</Panel>
				</Panel>
				<Panel class="FilterContainer">
					<Panel class="FilterIcon" />
					<!--<Label class="FilterLabel" text="#DOTA_HeroSelectorCategory_Cavern" />-->
					<Panel class="InfoContainer">
						<Label class="InfoLabel Label3" text="#DOTACavernCrawl_MapVariantClaimed_DescLine3" />
					</Panel>
				</Panel>
			</Panel>
		</Panel>

	</DOTAPopupUICavernCrawlClaimMapVariant>
</root>
