							</Panel>
						</Panel>

						<DropDown id="RankedLaneSelectionDD" disabled="true">
							<Label id="LaneNone" text="#DOTA_LaneSelectionNone" />
							<Label id="LaneOther" text="#DOTA_LaneSelectionOther" />
							<Label id="LaneSafe" text="#DOTA_LaneSelectionSafelane" />
							<Label id="LaneMid" text="#DOTA_LaneSelectionMidlane" />
							<Label id="LaneOff" text="#DOTA_LaneSelectionOfflane" />
						</DropDown>
						
					</Panel>
				</Panel>

				<Panel id="RankedSection" class="PlayTabSection TopBottomFlow">
